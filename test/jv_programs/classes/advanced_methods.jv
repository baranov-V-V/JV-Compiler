class A {
    public static void main() {
        Foo foo;
        foo = new Foo();
        Foo foo_inner;
        foo_inner = new Foo();
        int a;
        a = 3;
        foo.SetA(a);
        foo_inner.SetA(10);
        foo.SetFoo(foo_inner);
        a = 2;
        a = foo.GetA();
        foo.DoPrint();
        foo_inner.SetA(15);
        foo.DoPrint();
        println(foo.GetFoo().GetA());
    }
}

class Foo {
    int a;
    bool b;

    Foo foo;

    void SetA(int new_a) {
        a = new_a;
    }

    int GetA() {
        return a;
    }

    void DoPrint() {
        println(a);
        println(foo.GetA() + 1111);
    }

    void SetFoo(Foo new_foo) {
        foo = new_foo;
    }

    Foo GetFoo() {
        return foo;
    }
}